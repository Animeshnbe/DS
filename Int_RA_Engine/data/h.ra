LOAD Student
LOAD Marks
CROSSED_TABLE <- CROSS Student Marks
VALID_ROWS <- SELECT Student_Stud_Id == Marks_Stud_Id FROM CROSSED_TABLE
WITH_ADVISOR_2 <- SELECT Advisor_id == 2 FROM VALID_ROWS
WITH_ADVISOR_2_MARKS_MORE_THAN_90 <- SELECT Maths_marks > 90 FROM WITH_ADVISOR_2
RESULT <- PROJECT Student_Stud_Id FROM WITH_ADVISOR_2_MARKS_MORE_THAN_90
PRINT RESULT